{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    }
  ]
}